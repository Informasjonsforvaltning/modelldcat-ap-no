# -+-+- Some "general" classes, though modified for modelldcatno when necessary -+-+-+-

:Agent_Shape
    a sh:NodeShape ;
    sh:name "Agent"@en , "Aktør"@nb ;
    sh:property [
        sh:name "navn"@nb , "name"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path foaf:name ;
        sh:severity sh:Violation ;
        sh:message "Agent must have at least one name (foaf:name)."@en ,
            "Aktør må ha minst ett navn (foaf:name)."@nb ;
    ], [
        sh:name "organisasjonsidentifikator"@nb , "identifier"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:identifier ;
        sh:severity sh:Violation ;
        sh:message "Agent may have max. one identifier (dct:identifier)."@en ,
            "Aktør kan ha maks. én identifikator (dct:identifier)."@nb ;
    ],
        :TypeAgentProperty_Shape , # sjekker Kontrollert vokabular
    [ # sjekker multiplisitet
        sh:name "type (AKtør)"@nb , "type (Agent)"@en ;
        sh:path dct:type ;
        sh:maxCount 1 ;
        sh:message "Agent may have max. one agent type (dct:type)."@en ,
            "Aktør kan ha maks. én aktørtype (dct:type)."@nb ;
        sh:severity sh:Violation ;
    ] ;
    sh:targetClass foaf:Agent .

:ConceptScheme_Shape
    a sh:NodeShape ;
    sh:name "Thematic Scheme"@en , "Tematisk skjema"@nb ;
    sh:property [
        sh:name "tittel"@nb , "title"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation ;
        sh:message "Thematic scheme must have at least one title (dct:title)."@en ,
            "Tematisk skjema må ha minst én tittel (dct:title)."@nb ;
    ] ;
    sh:targetClass skos:ConceptScheme .

:LicenceDocument_Shape
    a sh:NodeShape ;
    sh:name "Licence Document"@en , "Lisensdokument"@nb ;
    sh:property
        :TypeLicenseDocumentProperty_Shape ; # sjekker kontrollert vokabular
    sh:targetClass dct:LicenseDocument .

:Location_Shape
    a sh:NodeShape ;
    sh:name "Location"@en , "Lokasjon"@nb ;
    sh:property [
        sh:name "område"@nb, "bounding box"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dcat:bbox ;
        sh:severity sh:Violation ;
        sh:message "Location may have max. one bouding box (dcat:bbox)."@en ,
            "Lokasjon kan ha maks. et område (dcat:bbox)."@nb ;
    ], [
        sh:name "geografisk midtpunkt"@nb , "centroid"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dcat:centroid ;
        sh:severity sh:Violation ;
        sh:message "Location may have max. one centroid (dcat:centroid)."@en ,
            "Lokasjon kan ha maks. ett geografisk midtpunnt (dcat:centroid)."@nb ;
    ], [
        sh:name "geometri"@nb , "geometry"@en ;
        sh:maxCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path locn:geometry ;
        sh:severity sh:Violation ;
        sh:message "Location may have max. one geometry (locn:geometry)."@en ,
            "Lokasjon kan ha maks. en geometri (locn:geometry)."@nb ;
    ] ;
    sh:targetClass dct:Location .

:PeriodOfTime_Shape
    a sh:NodeShape ;
    sh:name "Period of Time"@en , "Tidsrom"@nb ;
    sh:or (
      [
        sh:property [
          sh:name "sluttdato/tid"@nb , "end date"@en ;
          sh:minCount 1;
          sh:maxCount 1 ;
          sh:path dcat:endDate ;
          sh:severity sh:Violation ;
          sh:shape :DateOrDateTimeDataType_Shape ;
        ]
      ]
      [
        sh:property [
          sh:name "startdato/tid"@nb , "start date"@en ;
          sh:minCount 1;
          sh:maxCount 1 ;
          sh:path dcat:startDate ;
          sh:severity sh:Violation ;
          sh:shape :DateOrDateTimeDataType_Shape ;
        ]
      ]
    ) ;
    sh:message "Enten startdato eller sluttdato må ha verdi"@nb ,
      "Either start date or end date must have value"@en ;
    sh:targetClass dct:PeriodOfTime .

# -+-+- Some commonly used property-shapes, especially for use of Controlled Vocabularies: -+-+-

:DateOrDateTimeDataType_Shape
    a sh:NodeShape ;
    rdfs:comment "Date time date disjunction shape checks that a datatype property receives a date or a dateTime literal" ;
    sh:name "Date time date disjunction"@en , "Enten dato eller datotid"@nb ;
    sh:message "The values must be data typed as either xsd:date or xsd:dateTime."@en ,
       "Verdien må være oppgitt som enten xsd:date eller xsd:dateTime."@nb ;
    sh:or ([
            sh:datatype xsd:date
        ]
        [
            sh:datatype xsd:dateTime
        ]
    ) .

:FormatProperty_Shape
    a sh:PropertyShape ;
    sh:name "format"@nb, "format"@en ;
    sh:path dct:format ;
    # sh:class dct:MediaType ;
    sh:pattern "^http://publications.europa.eu/resource/authority/file-type/" ;
    sh:flags "i" ;
    sh:message "Medietype (dct:format) skal være en av verdiene fra http://publications.europa.eu/resource/authority/file-type/."@nb ;
    sh:severity sh:Violation ;
  .

:LanguageProperty_Shape
    a sh:PropertyShape ;
    sh:name "språk"@nb , "language"@en ;
    sh:path dct:language ;
    # sh:class dct:LinguisticSystem ;
    sh:pattern "^http://publications.europa.eu/resource/authority/language/" ;
    sh:flags "i" ;
    sh:message "Språkkoden (dct:language) skal være en av verdiene fra http://publications.europa.eu/resource/authority/language/."@nb ;
    sh:severity sh:Violation ;
  .

:LicenseProperty_Shape
    a sh:PropertyShape ;
    sh:name "lisens"@nb , "license"@en ;
    sh:path dct:license ;
    sh:pattern "^http://publications.europa.eu/resource/authority/licence" ;
    sh:flags "i";
    sh:message "Lisens (dct:license) skal være en av verdiene fra http://publications.europa.eu/resource/authority/licence/."@nb ;
    sh:severity sh:Violation ;
  .

:SpatialProperty_Shape
    a sh:PropertyShape ;
    sh:name "dekningsområde"@nb , "spatial"@en ;
    sh:path dct:spatial ;
    sh:or (
      [ sh:pattern "^http://publications.europa.eu/resource/authority/continent" ; sh:flags "i" ; ]
      [ sh:pattern "^http://publications.europa.eu/resource/authority/country" ; sh:flags "i" ; ]
      [ sh:pattern "^http://publications.europa.eu/resource/authority/place" ; sh:flags "i" ;]
      [ sh:pattern "^http://sws.geonames.org/" ; sh:flags "i" ; ]
    ) ;
    sh:message "Dekningsområde (dct:spatial) skal være minst en av verdiene fra http://publications.europa.eu/resource/authority/continent, http://publications.europa.eu/resource/authority/country, http://publications.europa.eu/resource/authority/place eller http://sws.geonames.org/."@nb ;
    sh:severity sh:Violation ;
  .

:StatusProperty_Shape
    a sh:PropertyShape ;
    sh:name "status"@nb , "status"@en ;
    sh:path adms:status ;
    sh:pattern "^http://purl.org/adms/status/" ;
    sh:flags "i" ;
    sh:message "Status (adms:status) skal være en av verdiene fra http://purl.org/adms/status/."@nb ;
    sh:severity sh:Violation ;
  .

:TypeAgentProperty_Shape
    a sh:PropertyShape ;
    sh:name "type (Aktør)"@nb , "type (Agent)"@en ;
    sh:path dct:type ;
    sh:pattern "^http://purl.org/adms/publishertype/" ;
    sh:flags "i" ;
    sh:message "Aktørtype (dct:type) skal være en av verdiene fra http://purl.org/adms/publishertype/."@nb ;
    sh:severity sh:Violation ;
  .

:TypeLicenseDocumentProperty_Shape
    a sh:PropertyShape ;
    sh:name "type (Lisensdokument)"@nb , "type (License document)"@en ;
    sh:path dct:type ;
    sh:pattern "^http://purl.org/adms/licencetype/" ;
    sh:flags "i" ;
    sh:message "Lisensdokumenttype (dct:type) skal være en av verdiene fra http://purl.org/adms/licencetype/."@nb ;
    sh:severity sh:Violation ;
  .

:ThemePropertyLOS_Shape
    a sh:PropertyShape ;
    sh:name "tema (LOS)"@nb , "theme (Norwegian LOS)"@en ;
    sh:path dcat:theme ;
    sh:pattern "^https://psi.norge.no/los/tema/" ;
    sh:flags "i" ;
    sh:message "LOS (https://psi.norge.no/los/tema/) bør brukes for dcat:theme"@nb ;
    sh:severity sh:Warning ;
  .

#
# -+-+-+-+- From here, shapes specific for ModellDCAT-AP-NO -+-+-+-
#

:Abstraction_Shape
    a sh:NodeShape ;
    sh:name "Abstraction"@en , "Abstraksjon"@nb ;
    sh:subClassOf modelldcatno:Property ;
    sh:property [
        sh:name "er abstraksjon av"@nb , "is abstraction of"@en ;
        sh:maxCount 1 ;
        sh:subClassOf modelldcatno:hasType ;
        sh:path modelldcatno:isAbstractionOf ;
        sh:or ( [sh:class modelldcatno:ModelElement]
                [sh:class modelldcatno:Property] ) ; # v.1.1
        sh:severity sh:Violation ;
        sh:message "The property `is abstraction of` ((modelldcatno:isAbstractionOf) may have max. one value, whish mush be an instance of modelldcatno:ModelElement or modelldcatno:Property."@en ,
            "Egenskal `er abstraksjon av` (modelldcatno:isAbstractionOf) kan ha maks. ´én verdi, som skal være instans av enten modelldcatno:ModelElement eller modelldcatno:Property."
    ] ;
    sh:targetClass modelldcatno:Abstraction ;
  .

:Association_Shape
    a sh:NodeShape ;
    sh:name "Association"@en , "Assosiasjon"@nb ;
    sh:subClassOf modelldcatno:Property ;
    sh:property [
        sh:name "refererer til"@nb, "refers to"@en ;
        sh:maxCount 1;
        sh:subClassOf modelldcatno:hasType ;
        sh:path modelldcatno:refersTo ;
        sh:class modelldcatno:ModelElement ;
        sh:severity sh:Violation ;
        sh:message "The property `refers to` (modelldcatno:refersTo) may have max. one value, which must be an instance of modelldcatno:ModelElement."@en ,
        "Egenskap `refererer til` (modelldcatno:refersTo) kan ha maks. én verdi, som skal være instans av modelldcatno:ModelElement."@nb ;
    ] ;
    sh:targetClass modelldcatno:Association ;
  .

:Attribute_Shape
    a sh:NodeShape ;
    sh:name "Attribute"@en , "Attributt"@nb ;
    sh:subClassOf modelldcatno:Property ;

    ### sh:or under er for de tre egenskapene som er gjensidig utelukkende:
    sh:or (
        [
            sh:message "Egenskapene modelldcatno:hasDataType, modelldcatno:hasSimpleType og modelldcatno:containsObjectType er gjensidig utelukkende og kan ikke forekomme samtidig."@nb ;
            sh:and (
                [
                  sh:property [
                    sh:name "inneholder objekttype"@nb , "contains object type"@en ;
                    sh:maxCount 1 ;
                    sh:path modelldcatno:containsObjectType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:ObjectType ;
                    sh:message "Egenskap `inneholder objekttype` (modelldcatno:hasType) kan ha maks. én verdi, som skal være instans av modelldcatno:ObjectType."@nb ;
                  ] ;
                ]
                [
                  sh:property [
                    sh:name "har datatype"@nb , "has data type"@en ;
                    sh:maxCount 0 ;
                    sh:path modelldcatno:hasDataType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:DataType ;
                  ] ;
                ]
                [
                  sh:property [
                    sh:name "har enkeltype"@nb , "has simple type"@en ;
                    sh:maxCount 0 ;
                    sh:path modelldcatno:hasSimpleType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:SimpleType ;
                  ] ;
                ]
            );
        ]
        [
            sh:message "Egenskapene modelldcatno:hasDataType, modelldcatno:hasSimpleType og modelldcatno:containsObjectType er gjensidig utelukkende og kan ikke forekomme samtidig."@nb ;
            sh:and (
                [
                  sh:property [
                    sh:name "inneholder objekttype"@nb , "contains object type"@en ;
                    sh:maxCount 0 ;
                    sh:path modelldcatno:containsObjectType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:ObjectType ;
                  ] ;
                ]
                [
                  sh:property [
                    sh:name "har datatype"@nb , "has data type"@en ;
                    sh:maxCount 1 ;
                    sh:path modelldcatno:hasDataType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:DataType ;
                    sh:message "Egenskap `har datatype` (modelldcatno:hasDataType) kan ha maks. én verdi, som skal være instans av modelldcatno:DataType."@nb ;
                  ] ;
                ]
                [
                  sh:property [
                    sh:name "har enkeltype"@nb , "has simple type"@en ;
                    sh:maxCount 0 ;
                    sh:path modelldcatno:hasSimpleType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:SimpleType ;
                  ] ;
                ]
            );
        ]
        [
            sh:message "Egenskapene modelldcatno:hasDataType, modelldcatno:hasSimpleType og modelldcatno:containsObjectType er gjensidig utelukkende og kan ikke forekomme samtidig."@nb ;
            sh:and (
                [
                  sh:property [
                    sh:name "inneholder objekttype"@nb , "contains object type"@en ;
                    sh:maxCount 0 ;
                    sh:path modelldcatno:containsObjectType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:ObjectType ;
                  ] ;
                ]
                [
                  sh:property [
                    sh:name "har datatype"@nb , "has data type"@en ;
                    sh:maxCount 0 ;
                    sh:path modelldcatno:hasDataType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:DataType ;
                  ] ;
                ]
                [
                  sh:property [
                    sh:name "har enkeltype"@nb , "has simple type"@en ;
                    sh:maxCount 1 ;
                    sh:path modelldcatno:hasSimpleType ;
                    sh:subClassOf modelldcatno:hasType ;
                    sh:class modelldcatno:SimpleType ;
                    sh:message "Egenskap `har enkeltype` (modelldcatno:hasSimpleType) kan ha maks. én verdi, som skal være instans av modelldcatno:SimpleType."@nb ;
                  ] ;
                ]
            );
        ]
    );

    sh:property [
        sh:name "har verdier fra"@nb , "has value from"@en ;
        sh:maxCount 1;
        sh:subClassOf modelldcatno:hasType ;
        sh:path modelldcatno:hasValueFrom ;
        sh:class modelldcatno:CodeList ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `har verdier fra` (modelldcatno:hasValueFrom) kan ha maks. én verdi, som skal være instans av modelldcatno:CodeList."@nb ;
      ] ;
    sh:targetClass modelldcatno:Attribute ;
  .

:Begrensningsregel_Shape # hele klassen er ny i v.1.1
    a sh:NodeShape ;
    sh:name "Constraint rule"@en , "Begrensningsregel"@nb ;
    sh:property [
        sh:name "begrensning"@nb , "constraint"@en ;
        sh:minCount 1 ;
        sh:path modelldcatno:constraint;
        sh:or (
          [ sh:class modelldcatno:ModelElement ]
          [ sh:class modelldcatno:Property ]
        ) ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `begrensning` (modelldcatno:constraint) skal ha minst én verdi, som skal være instans av modelldcatno:ModelElement eller modelldcatno:Property."@nb ;
    ], [
        sh:name "identifikator"@nb , "identifier"@en ;
        sh:maxCount 1 ;
        sh:path dct:identifier;
        sh:nodeKind sh:Literal ;
        sh:severity sh:Violation ;
        sh:message "Begresningsregel kan ha maks. én identifikator (dct:identifier)."@nb ;
    ], [
        sh:name "tilhører modul"@nb , "belongs to module"@en ;
        sh:path modelldcatno:belongsToModule;
        sh:nodeKind sh:IRIOrLiteral ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `tilhører modul` (modelldcatno:belongsToModule skal være enten Literal eller IRI."@nb ;
    ] ;
    sh:or ( # minst en av følgende to egenskaper må være med
      [ sh:property [
            sh:name "begrensningsuttrykk"@nb , "constraint expression"@en ;
            sh:path modelldcatno:constraintExpression;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:message "Begrensningsregel (med unntak av subklassene modelldcatno:Xor og modelldcatno:OR) må som minimum ha en tittel (dct:title) eller et begrensningsuttrykk (modelldcatno:constraintExpression)."@nb ;
        ]
      ]
      [ sh:property [
          sh:name "tittel"@nb , "title"@en ;
          sh:path dct:title;
          sh:minCount 1 ;
          sh:nodeKind sh:Literal ;
          sh:severity sh:Violation ;
          sh:message "Begrensningsregel (med unntak av subklassene modelldcatno:Xor og modelldcatno:OR) må som minimum ha en tittel (dct:title) eller et begrensningsuttrykk (modelldcatno:constraintExpression)."@nb ;
        ]
      ]
    ) ;
    sh:targetClass modelldcatno:Begrensningsregel ;
    .

# Lager en egen shape for modelldcatno:Or og modelldcatno:Xor
# pga. at min-krav som gjelder for superklassen ikke gjelder her
:OrXor_Shape # hele klassen er ny i v.1.1
    a sh:NodeShape ;
    sh:name "OR/XOR"@en , "Eller / Enten eller"@nb ;
    sh:property [
        sh:name "begrensning"@nb , "constraint"@en ;
        sh:minCount 1 ;
        sh:path modelldcatno:constraint;
        sh:or (
          [ sh:class modelldcatno:ModelElement ]
          [ sh:class modelldcatno:Property ]
        ) ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `begrensning` (modelldcatno:constraint) skal ha minst én verdi, som skal være instans av modelldcatno:ModelElement eller modelldcatno:Property."@nb ;
    ], [
        sh:name "identifikator"@nb , "identifier"@en ;
        sh:maxCount 1 ;
        sh:path dct:identifier;
        sh:nodeKind sh:Literal ;
        sh:severity sh:Violation ;
        sh:message "OR/XOR kan ha maks. én identifikator (dct:identifier)."@nb ;
    ], [
        sh:name "tilhører modul"@nb , "title"@en ;
        sh:path modelldcatno:belongsToModule;
        sh:nodeKind sh:IRIOrLiteral ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `tilhører modul` (belongsToModule) kan bare være enten Literal eller IRI."@nb ;
    ] ;
    sh:property [
        sh:name "begrensningsuttrykk"@nb , "constraint expression"@en ;
        sh:path modelldcatno:constraintExpression;
        sh:nodeKind sh:Literal ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `begrensningsuttrykk` (modelldcatno:constraintExpression) kan bare være Literal."@nb ;
        ] ;
    sh:property [
        sh:name "tittel"@nb , "title"@en ;
        sh:path dct:title;
        sh:nodeKind sh:Literal ;
        sh:severity sh:Violation ;
        sh:message "Egenskap `tittel` (dct:title) kan bare være Literal."@nb ;
        ] ;
    sh:targetClass modelldcatno:Or, modelldcatno:Xor ;
    .

:Catalog_Shape
    a sh:NodeShape ;
    sh:name "Catalog"@en , "Modellkatalog"@nb ;
    sh:property [
        sh:name "beskrivelse"@nb , "description"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:description ;
        sh:severity sh:Violation ;
        sh:message "En katalog skal ma minst én beskrivelse (dct:description)."@nb ;
    ], [
        sh:name "identifikator"@nb , "identifier"@en ;
        sh:nodeKind sh:Literal ;
        sh:maxCount 1 ;
        sh:minCount 1 ;
        sh:path dct:identifier ;
        sh:severity sh:Violation ;
        sh:message "En katalog skal én og bare én identifikator (dct:identifier)."@nb ;
    ], [
        sh:name "tittel"@nb , "title"@en ;
        sh:minCount 1 ;
        sh:nodeKind sh:Literal ;
        sh:path dct:title ;
        sh:severity sh:Violation ;
        sh:message "En katalog skal ma minst én tittel (dct:title)."@nb ;
    ], [
